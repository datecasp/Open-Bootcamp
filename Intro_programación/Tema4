while numeroWhile<3 {
    numeroWhile ++;
    System.out.println(numeroWhile);
}

do {
    numeroWhile = 4;
} while numeroWhile<3;

for (numeroFor = 0; numeroFor<=3;numeroFor++){
    System.out.prinln(numeroFor);
}

string estacion = "Verano";

switch(estacion){
    case ("Verano"):
    case("Primavera"):
    case ("Invierno"):
    case ("Otoño"):
        System.out.println(estacion);
        break;
    default:
        System.out.println("No es una estación");
}